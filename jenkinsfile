pipeline {
    agent any

    stages {
        stage('pull') {
            steps {
                git branch: 'main', url: 'https://github.com/narwadesonali7/jenkinfile-repo-declarative.git'
            }
        }
         stage('build') {
            steps {
                echo 'build succed'
            }
        }
        stage('test') {
            steps {
                echo 'test succed'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy succed'
            }
        }
    }
}